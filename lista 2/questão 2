int main(){
    float nota;
    printf("insira a nota do aluno \n");
    scanf("%f", &nota);
    if(nota>=0 && nota<=100){
    if(nota>=90 && nota<=100)
    {printf("A");}
    else{if(nota>=80)
        printf("B");
        else{if(nota>=70)
            printf("C");
            else{if(nota>=50)
                printf("D");
                else{printf("E");}}}}}
    return 0;
}
