int main()
{
    float x, y, aux1, aux2, aux3;
    printf("insira as coordenadas do ponto (x,y) \n");
    fflush(stdin);
    scanf("%f %f", &x, &y);
    aux1=(x*x)/16;
    aux2=(y*y)/25;
    aux3=(aux1+aux2);
    if (aux3>=1)
    {
        printf("\no ponto (%.0f,%.0f) esta contido na elipse", x, y);
    }
    else
    {
        printf("\no ponto (%.0f,%.0f) nao esta contido na elipse", x, y);
    }

return 0;
}
